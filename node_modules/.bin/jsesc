#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/codes/porto/portfolio real/portofolio/node_modules/jsesc/bin/node_modules:/mnt/d/codes/porto/portfolio real/portofolio/node_modules/jsesc/node_modules:/mnt/d/codes/porto/portfolio real/portofolio/node_modules:/mnt/d/codes/porto/portfolio real/node_modules:/mnt/d/codes/porto/node_modules:/mnt/d/codes/node_modules:/mnt/d/node_modules:/mnt/d/codes/porto/portfolio real/portofolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/codes/porto/portfolio real/portofolio/node_modules/jsesc/bin/node_modules:/mnt/d/codes/porto/portfolio real/portofolio/node_modules/jsesc/node_modules:/mnt/d/codes/porto/portfolio real/portofolio/node_modules:/mnt/d/codes/porto/portfolio real/node_modules:/mnt/d/codes/porto/node_modules:/mnt/d/codes/node_modules:/mnt/d/node_modules:/mnt/d/codes/porto/portfolio real/portofolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../jsesc/bin/jsesc" "$@"
fi
